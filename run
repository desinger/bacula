
docker run --name bacula -p 9101:9101 -p 9102:9102 -p 9103:9103 -it -e TZ=Europe/London --net=bru -v /etc/bacula/bacula-dir.conf:/etc/bacula/bacula-dir.conf -v /etc/bacula/bacula-sd.conf:/etc/bacula/bacula-sd.conf -v /etc/bacula/bconsole.conf:/etc/bacula/bconsole.conf -v /etc/bacula/bacula-fd.conf:/etc/bacula/bacula-fd.conf stanimirvelikov/bacula bash

